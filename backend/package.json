{
	"name": "cafe-pos-backend",
	"version": "1.0.0",
	"description": "Server for Cafe POS app",
	"main": "./src/app.js",
	"scripts": {
		"test": "jest",
		"coverage": "jest --coverage",
		"start": "node ./src/server.js",
		"dev": "nodemon ./src/server.js",
		"lint": "eslint .",
		"lint:fix": "eslint --fix .",
		"prettier": "prettier --check .",
		"prettier:fix": "prettier --write .",
		"migrate": "npx sequelize-cli db:migrate",
		"migrate:undo": "npx sequelize-cli db:migrate:undo:all",
		"seed": "npx sequelize-cli db:seed:all",
		"seed:undo": "npx sequelize-cli db:seed:undo:all"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sanfarish/cafe-pos.git"
	},
	"keywords": [
		"point-of-sales"
	],
	"author": "Faris Hasan",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/sanfarish/cafe-pos/issues"
	},
	"homepage": "https://github.com/sanfarish/cafe-pos#readme",
	"dependencies": {
		"cors": "2.8.5",
		"dotenv": "16.4.5",
		"express": "4.21.1",
		"morgan": "1.10.0",
		"mysql2": "3.11.3",
		"pg": "8.13.1",
		"pg-hstore": "2.3.4",
		"sequelize": "6.37.4",
		"swagger-ui-express": "5.0.1"
	},
	"devDependencies": {
		"@eslint/js": "9.13.0",
		"@types/express": "5.0.0",
		"@types/jest": "29.5.14",
		"eslint": "9.13.0",
		"eslint-config-prettier": "9.1.0",
		"globals": "15.11.0",
		"jest": "29.7.0",
		"nodemon": "3.1.7",
		"prettier": "3.3.3",
		"sequelize-cli": "6.6.2",
		"supertest": "7.0.0"
	}
}
